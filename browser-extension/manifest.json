{
    "manifest_version": 2,
    "name": "Bookmarks-Sync",
    "author": "K!2023",
    "version": "1.0",

    "description": "",

    "permissions": ["bookmarks", "storage"],

    "options_ui": {
        "page": "settings/settings.html"
    },

    "browser_action": {
        "default_title": "Bookmarks-Sync",
        "default_popup": "popup/popup.html"
    },

    "background": {
        "scripts": ["background.js"],
        "persistent": false,
        "type": "module"
    }
}
